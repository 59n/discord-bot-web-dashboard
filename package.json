{
  "name": "discord-bot-controller",
  "version": "2.0.0",
  "description": "Modern Discord bot with React dashboard",
  "main": "bot.js",
  "scripts": {
    "start": "node bot.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon bot.js",
    "client": "cd client && npm start",
    "install-client": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run install-client && npm run build",
    "generate-test-data": "node scripts/generateTestData.js"
  },
  "dependencies": {
    "@mui/x-date-pickers": "^8.4.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "node-cron": "^4.0.7",
    "qrcode": "^1.5.3",
    "recharts": "^2.15.3",
    "rss-parser": "^3.13.0",
    "socket.io": "^4.7.4",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
